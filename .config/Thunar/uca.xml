<action>
	<icon>image-optimizer</icon>
	<name>Optimize with jpegoptim</name>
	<unique-id>1571523758714138-4</unique-id>
	<command>for file in %F; do jpegoptim --strip-all -of &quot;$file&quot;; done</command>
